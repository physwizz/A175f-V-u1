ifneq (,$(filter m,$(CONFIG_SENSORS_SX9385) $(CONFIG_SENSORS_SX9385_SUB) $(CONFIG_SENSORS_SX9385_SUB2) $(CONFIG_SENSORS_SX9385_WIFI)))
	obj-m += sx9385.o
