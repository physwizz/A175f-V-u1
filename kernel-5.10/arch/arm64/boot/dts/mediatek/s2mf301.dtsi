/delete-node/ &s2mf301;

&pio {
	i2c0_pins: i2c0_pins {
			pins_cmd_dat {
			pinmux = <PINMUX_GPIO130__FUNC_SCL0>,
					<PINMUX_GPIO131__FUNC_SDA0>;
			bias-disable;
			mediatek,drive-strength-adv = <00>;
			drive-strength = <3>;
		};
	};
};

&i2c0 {
	#address-cells = <1>;
	#size-cells = <0>;
	clock-frequency = <400000>;
	mediatek,use-open-drain;
	s2mf301: s2mf301@3A {
		status = "okay";
		compatible = "samsung,s2mf301mfd";
		reg = <0x3A>;
		pinctrl-names = "default";
		pinctrl-0 = <&if_pmic_irq &i2c0_pins>;
		s2mf301,irq-gpio = <SEC_GPIO_REF(AP,pio,7) 1>;
		s2mf301,wakeup;
	};
};

